$("#exportButton").click(function(){
    
  html2canvas(document.querySelector("#x"),{scrollY: -window.scrollY}).then(canvas => {  
    var dataURL = canvas.toDataURL();
     var margins = {
                top: 100,
                bottom: 290,
                left: 179,
                width: 530
            };
    var pdf = new jsPDF('p', 'mm');
    var  date = new Date();
    var year = date.getFullYear();
    var month = date.getMonth() + 1;
    var day = date.getDate();
    var src= 'data:image/png;base64,'
     pdf.setFontSize(8);
    pdf.text(160, 63,"DATE PRINTED: "+ month + "/" + day + "/" + year);
     var actorName = $("#actor").val();
    
     var actorSurname = $("#surname").val();
    pdf.text(20, 63,"PRINTED BY: "+actorName+" "+actorSurname);
    pdf.addImage(src, 'PNG', 5, 3, 210, 57);//addImage(image, format, x-coordinate, y-coordinate, width, height240, 210, 57)
    const imgWidth = 350;
  const pageHeight = 300;
  const imgHeight = (canvas.height * imgWidth) / canvas.width;
  let heightLeft = imgHeight;
  let position = 55;
  heightLeft -= pageHeight;
   
  pdf.addImage(dataURL, 'PNG', -60, position, imgWidth, imgHeight, '', 'FAST');
  while (heightLeft >= 0) {
    position = heightLeft - imgHeight +55;
    pdf.addPage();
    pdf.addImage(dataURL, 'PNG', -60, position, imgWidth, imgHeight, '', 'FAST');
    heightLeft -= pageHeight;
    
  }  pdf.setFontSize(12);
    pdf.text(87.5, margins.bottom,"***END OF REPORT***");
    pdf.save("SalesReport.pdf");
  });
});
